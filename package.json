{
	"name": "audify",
	"version": "1.0.0",
	"description": "Play/Stream PCM audio data & Encode/Decode Opus to PCM audio data",
	"main": "index.js",
	"types": "index.d.ts",
	"scripts": {
		"docs": "typedoc --out docs . --ignoreCompilerErrors --includeDeclarations --exclude \"**/node_modules/**\" --mode file --theme node_modules/eledoc/bin/default/ --hideGenerator --name \"Audify.js\"",
		"build": "cmake-js build",
		"postinstall": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/almogh52/audify.git"
	},
	"keywords": [
		"play",
		"stream",
		"pcm",
		"encode",
		"decode",
		"opus",
		"alsa",
		"jack",
		"pulseaudio",
		"oss",
		"coreaudio",
		"asio",
		"wasapi",
		"record",
		"input",
		"output",
		"speaker",
		"sound",
		"audio",
		"music",
		"voip",
		"headphones"
	],
	"author": "Almog Hamdani",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/almogh52/audify/issues"
	},
	"homepage": "https://github.com/almogh52/audify#readme",
	"dependencies": {
		"bindings": "^1.5.0",
		"cmake-js": "^6.0.0",
		"node-addon-api": "^2.0.0"
	},
	"devDependencies": {
		"@types/node": "^12.12.17",
		"eledoc": "^0.2.1",
		"typedoc": "^0.15.4"
	}
}
